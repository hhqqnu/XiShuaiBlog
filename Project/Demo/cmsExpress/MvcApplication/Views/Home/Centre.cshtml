@using CMSExpress.AppServices.Mvc.Extensions;
@{
    Layout = null;
}
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>工作台 - @Html.ApplicationName()</title>
    @Html.Partial("_HeaderPartial")
    <script src="@Url.Content("~/scripts/bpm.core.js")" type="text/javascript"></script>
    <script src="@Url.Content("~/scripts/bpm.main.js")" type="text/javascript"></script>
</head>
<body class="easyui-layout">
    <div data-options="region:'north'" style="overflow:hidden;height:40px;">
        <div class="pageHeader"></div>
    </div>
    <div data-options="region:'west',split:true" title="工作台导航" style="width:180px;">
        <div id="treeNavigator"></div>
    </div>
    <div data-options="region:'center'" style="background:#fafafa;overflow:hidden">
        <div class="easyui-tabs" data-options="fit:true,border:false">
		</div>
    </div>
    <div data-options="region:'south'" style="overflow:hidden;height:25px;">
        <div class="pageFooter">
            @Html.Copyright()
        </div>
    </div>
    <script type="text/javascript">
        $(function () {
            bpm.main.addTabItem("菜单管理", "admin/permission/index?");
            //bpm.main.alert("hello");
        });
    </script>
</body>
</html>